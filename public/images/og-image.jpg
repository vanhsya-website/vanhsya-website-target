  <text x="560" y="440" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14" font-weight="bold">CV Builder</text>
  
  <rect x="640" y="420" width="130" height="30" rx="15" fill="rgba(255,255,255,0.2)"/>
  <text x="705" y="440" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Referral System</text>
  
